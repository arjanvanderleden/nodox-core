    "guid": "0.0.12"
  },
  "devDependencies": {
    "@types/chai": "^4.0.3",
    "@types/mocha": "^2.2.41",
    "chai": "^4.1.1",
    "coveralls": "^2.13.1",
    "del-cli": "^1.1.0",
